{% extends "layouts/base.html" %}

{% set filename = 'index' %}

{% block title %} Dashboard {% endblock %}

{% block stylesfirst %}
{% endblock %}

{% block styles %}
<link rel="stylesheet" href="static/vendors/iconly/bold.css">

{% endblock %}

{% block title_page %} Halaman Utama {% endblock %}

{% block item_page %} {% endblock %}

{% block content %}
<section class="row">
  <div class="col-12 col-lg-9">
    <div class="row">
      <div class="col-6 col-lg-3 col-md-6">
        <div class="card">
          <div class="card-body px-3 py-4-5">
            <div class="row">
              <div class="col-md-4">
                <div class="stats-icon red">
                  <i class="iconly-boldUser"></i>
                </div>
              </div>
              <div class="col-md-8">
                <h6 class="text-muted font-semibold">Data Pasien</h6>
                <h6 class="font-extrabold mb-0">{{ dataPasien }} Orang</h6>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="col-6 col-lg-3 col-md-6">
        <div class="card">
          <div class="card-body px-3 py-4-5">
            <div class="row">
              <div class="col-md-4">
                <div class="stats-icon blue">
                  <i class="iconly-boldUser"></i>
                </div>
              </div>
              <div class="col-md-8">
                <h6 class="text-muted font-semibold">Antrian Poli Umum</h6>
                <h6 class="font-extrabold mb-0">{{ dataPoliUmum}} Orang</h6>
              </div>
            </div>
          </div>
        </div>
      </div>


      <div class="col-6 col-lg-3 col-md-6">
        <div class="card">
          <div class="card-body px-3 py-4-5">
            <div class="row">
              <div class="col-md-4">
                <div class="stats-icon green">
                  <i class="iconly-boldUser"></i>
                </div>
              </div>
              <div class="col-md-8">
                <h6 class="text-muted font-semibold">Antrian Poli Gigi</h6>
                <h6 class="font-extrabold mb-0">{{ dataPoliGigi}} Orang</h6>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="col-6 col-lg-3 col-md-6">
        <div class="card">
          <div class="card-body px-3 py-4-5">
            <div class="row">
              <div class="col-md-4">
                <div class="stats-icon red">
                  <i class="iconly-boldUser"></i>
                </div>
              </div>
              <div class="col-md-8">
                <h6 class="text-muted font-semibold">Poli Spesialis</h6>
                <h6 class="font-extrabold mb-0">{{ dataPoliSpesialis }}</h6>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-12 col-lg-3">
    <div class="col-7 col-lg-9">
      <div class="card">
        <div class="card-body px-3 py-4-5">
          <div class="row">
            <div class="col-md-4">
              <div class="stats-icon red">
                <i class="iconly-boldUser"></i>
              </div>
            </div>
            <div class="col-md-8">
              <h6 class="text-muted font-semibold">Poli KIA</h6>
              <h6 class="font-extrabold mb-0">{{ dataPoliKia }}</h6>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

{% endblock %}

{% block js %}
{% endblock %}